datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Sensors {
  id        Int     @id @default(autoincrement())
  temp      String
  alt       String
  press     String      
  heat      String
  humid     String
  lux       String
  flame     String
  createdAt DateTime @updatedAt
}