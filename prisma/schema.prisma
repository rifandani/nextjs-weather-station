datasource db {
  provider = "sqlite"
  url      = "file:./sqlitee.db"
}

generator client {
  provider = "prisma-client-js"
}

model Sensor {
  id        Int     @id @default(autoincrement())
  temp      Float
  alt       Float
  press     Float
  heat      Float
  humid     Float
  lux       Float
  flame     Int
  time      String
}